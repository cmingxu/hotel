<script type='text/javascript'>
  show_kindeditor("kindeditor");
</script>
<tr class='tit'>
  <td>
    <%= f.label :icon,"图片"%>
  </td>
  <td>
     <%= image_tag(f.object.icon.url(:s)) if  f.object.icon%><%= f.file_field :icon%>
  </td>
</tr>

<tr class='tit'>
  <td>
    <%= f.label :cate,"类别"%>
  </td>
  <td>
    <%= f.select :cate,Room::ROOM_CATE.invert%>
  </td>
</tr>

<tr class='tit'>
  <td>
    <%= f.label :name ,"名称"%>
  </td>
  <td>
    <%= f.text_field :name%>
  </td>
</tr>
<tr>
  <td class='tit'>
    <%= f.label :price ,"价格"%>
  </td>
  <td>
    <%= f.text_field :price %>
  </td>
</tr>

<tr>
  <td class='tit'>
    <%= f.label :quantity,"数量" %>
  </td>
  <td>
    <%= f.text_field :quantity %>
  </td>
</tr>


<tr>
  <td class='tit'>
    <%= f.label :desc,"描述"%>
  </td>
  <td>
    <%= f.text_field :desc,:id => "kindeditor"%>
  </td>
</tr>


